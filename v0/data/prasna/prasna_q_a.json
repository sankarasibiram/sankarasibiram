    },
    {
        "sloka_number": "66",
        "question_number": 181,
        "prasna": "मोक्षश्च कः",
        "utharam": "अविद्यास्तमयः"
    },
    {
        "sloka_number": "66",
        "question_number": 182,
        "prasna": "कः सर्ववेदभूः",
        "utharam": "अथ च ॐ"
    }
]