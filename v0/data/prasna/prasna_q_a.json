    },
    {
        "sloka_number": "48",
        "question_number": 123,
        "prasna": "कः सुकृती",
        "utharam": "श्लाघ्यते च यः सद्भिः"
    },
    {
        "sloka_number": "49",
        "question_number": 124,
        "prasna": "गृहमेधिनश्च मित्रं किं",
