    },
    {
        "sloka_number": "42",
        "question_number": 103,
        "prasna": "पातकं च किम्",
        "utharam": "हिंसा"
    },
    {
        "sloka_number": "42",
        "question_number": 104,
        "prasna": "को हि भगवत्प्रियः स्यात्",
