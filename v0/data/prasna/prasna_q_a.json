[
    {
        "sloka_number": "2",
        "question_number": 1,
        "prasna": "किमुपादेयम्",
        "utharam": "गुरुवचनम्"
    },
    {
        "sloka_number": "2",
        "question_number": 2,
        "prasna": "हेयमपि किम् ",
        "utharam": "अकार्यम्"
    },
    {
        "sloka_number": "2",
        "question_number": 3,
        "prasna": "को गुरुः",
        "utharam": "अधिगततत्त्वः शिष्यहितायोद्यतः सततम्"
    },
    {
        "sloka_number": "3",
        "question_number": 4,
        "prasna": "त्वरितं किं कर्तव्यं विदुषाम्",
        "utharam": "संसारसन्ततिच्छेदः"
    },
    {
        "sloka_number": "3",
        "question_number": 5,
        "prasna": "किं मोक्षतरोर्बीजम्",
        "utharam": "सम्यक्ज्ञानं क्रियासिद्धम्"
    },
    {
        "sloka_number": "4",
        "question_number": 6,
        "prasna": "कः पथ्यतरः",
        "utharam": "धर्मः"
    },
    {
        "sloka_number": "4",
        "question_number": 7,
        "prasna": "कः शुचिरिह",
        "utharam": "यस्य मानसं शुद्धम्"
    },
    {
        "sloka_number": "4",
        "question_number": 8,
        "prasna": "कः पण्डितः",
        "utharam": "विवेकी"
    },
    {
        "sloka_number": "4",
        "question_number": 9,
        "prasna": "किं विषम्",
        "utharam": "अवधीरणा गुरुषु"
    },
    {
        "sloka_number": "5",
        "question_number": 10,
        "prasna": "किं संसारे सारम्",
        "utharam": "बहुशोऽपि विचिन्त्यमानमिदमेव"
    },
    {
        "sloka_number": "5",
        "question_number": 11,
        "prasna": "किं मनुजेष्विष्टतमम्",
        "utharam": "स्वपरहितायोद्यतं जन्म"
    },
    {
        "sloka_number": "6",
        "question_number": 12,
        "prasna": "मदिरेव मोहजनकः कः",
        "utharam": "स्नेहः"
    },
    {
        "sloka_number": "6",
        "question_number": 13,
        "prasna": "के च दस्यवः",
        "utharam": "विषयाः"
    },
    {
        "sloka_number": "6",
        "question_number": 14,
        "prasna": "का भववल्ली",
        "utharam": "तृष्णा"
    },
    {
        "sloka_number": "6",
        "question_number": 15,
        "prasna": "को वैरी",
        "utharam": "यस्त्वनुद्योगः"
    },
    {
        "sloka_number": "7",
        "question_number": 16,
        "prasna": "कस्माद्भयमिह",
        "utharam": "मरणात्"
    },
    {
        "sloka_number": "7",
        "question_number": 17,
        "prasna": "अन्धादिह को विशिष्यते",
        "utharam": "रागी"
    },
    {
        "sloka_number": "7",
        "question_number": 18,
        "prasna": "कः शूरः",
        "utharam": "यो ललनालोचनबाणैर्न च व्यधितः"
    },
    {
        "sloka_number": "8",
        "question_number": 19,
        "prasna": "पातुं कर्णाञ्जलिभिः किममृतमिह युज्यते",
        "utharam": "सदुपदेशः"
    },
    {
        "sloka_number": "8",
        "question_number": 20,
        "prasna": "किं गुरुताया मूलम्",
        "utharam": "यदेतदप्रार्थनं नाम"
    },
    {
        "sloka_number": "9",
        "question_number": 21,
        "prasna": "किं गहनम्",
        "utharam": "स्त्रीचरितम्"
    },
    {
        "sloka_number": "9",
        "question_number": 22,
        "prasna": "कश्चतुरः",
        "utharam": "यो न खण्डितस्तेन"
    },
    {
        "sloka_number": "9",
        "question_number": 23,
        "prasna": "किं दुःखम्",
        "utharam": "अ​सन्तोषः"
    },
    {
        "sloka_number": "9",
        "question_number": 24,
        "prasna": "किं लाघवम्",
        "utharam": "अधमतो याच्ञा"
    },
    {
        "sloka_number": "10",
        "question_number": 25,
        "prasna": "किं जीवितम्",
        "utharam": "अनवद्यम्"
    },
    {
        "sloka_number": "10",
        "question_number": 26,
        "prasna": "किं जाड्यम्",
        "utharam": "पाठतोऽप्यनभ्यासः"
    },
    {
        "sloka_number": "10",
        "question_number": 27,
        "prasna": "को जागर्ति",
        "utharam": "विवेकी"
    },
    {
        "sloka_number": "10",
        "question_number": 28,
        "prasna": "का निद्रा",
        "utharam": "मूढता जन्तोः"
    },
    {
        "sloka_number": "11",
        "question_number": 29,
        "prasna": "नलिनीदलगतजलवत्तरलं किम्",
        "utharam": "यौवनं धनं चायुः"
    },
    {
        "sloka_number": "11",
        "question_number": 30,
        "prasna": "कथय पुनः के शशिनः किरणसमाः",
        "utharam": "सज्जना एव"
    },
    {
        "sloka_number": "12",
        "question_number": 31,
        "prasna": "को नरकः",
        "utharam": "परवशता"
    },
    {
        "sloka_number": "12",
        "question_number": 32,
        "prasna": "किं सौख्यम्",
        "utharam": "सर्वसङ्गविरतिर्या"
    },
    {
        "sloka_number": "12",
        "question_number": 33,
        "prasna": "किं साध्यम्",
        "utharam": "भूतहितम्"
    },
    {
        "sloka_number": "12",
        "question_number": 34,
        "prasna": "प्रियं च किं प्राणिनाम्",
        "utharam": "असवः"
    },
    {
        "sloka_number": "13",
        "question_number": 35,
        "prasna": "कोऽनर्थफलः",
        "utharam": "मानः"
    },
    {
        "sloka_number": "13",
        "question_number": 36,
        "prasna": "का सुखदा",
        "utharam": "साधुजनमैत्री"
    },
    {
        "sloka_number": "13",
        "question_number": 37,
        "prasna": "सर्वव्यसनविनाशे को दक्षः",
        "utharam": "सर्वथा त्यागी"
    },
    {
        "sloka_number": "14",
        "question_number": 38,
        "prasna": "किं मरणम्",
        "utharam": "मूर्खत्वं"
    },
    {
        "sloka_number": "14",
        "question_number": 39,
        "prasna": "किं चानर्घम्",
        "utharam": "यदवसरे दत्तम्"
    },
    {
        "sloka_number": "14",
        "question_number": 40,
        "prasna": "आमरणात् किं शल्यम्",
        "utharam": "प्रच्छन्नं यत्कृतं पापम्"
    },
    {
        "sloka_number": "15",
        "question_number": 41,
        "prasna": "कुत्र विधेयो यत्नः",
        "utharam": "विद्याभ्यासे सदौषधे दाने"
    },
    {
        "sloka_number": "15",
        "question_number": 42,
        "prasna": "अवधीरणा क्व कार्या",
        "utharam": "खलपरयोषित्परधनेषु"
    },
    {
        "sloka_number": "16",
        "question_number": 43,
        "prasna": "काहर्निशमनुचिन्त्या",
        "utharam": "संसारासारता न तु प्रमदा"
    },
    {
        "sloka_number": "16",
        "question_number": 44,
        "prasna": "का प्रेयसी विधेया",
        "utharam": "करुणा दीनेषु सज्जने मैत्री"
    },
    {
        "sloka_number": "17",
        "question_number": 45,
        "prasna": "कण्ठगतैरप्यसुभिः कस्य ह्यात्मा न शक्यते जेतुम्",
        "utharam": "मूर्खस्य शङ्कितस्य च विषादिनो वा कृतघ्नस्य"
    },
    {
        "sloka_number": "18",
        "question_number": 46,
        "prasna": "कः साधुः",
        "utharam": "सद्वृत्तः"
    },
    {
        "sloka_number": "18",
        "question_number": 47,
        "prasna": "कमधममाचक्षते",
        "utharam": "असद्वृत्तम्"
    },
    {
        "sloka_number": "18",
        "question_number": 48,
        "prasna": "केन जितं जगदेतत्",
        "utharam": "सत्यतितिक्षावता पुंसा"
    },
    {
        "sloka_number": "19",
        "question_number": 49,
        "prasna": "कस्मै नमांसि देवाः कुर्वन्ति",
        "utharam": "दयाप्रधानाय"
    },
    {
        "sloka_number": "19",
        "question_number": 50,
        "prasna": "कस्मादुद्वेगः स्यात् सुधियः",
        "utharam": "संसारारण्यतः"
    },
    {
        "sloka_number": "20",
        "question_number": 51,
        "prasna": "कस्य वशे प्राणिगणः",
        "utharam": "सत्यप्रियभाषिणो विनीतस्य"
    },
    {
        "sloka_number": "20",
        "question_number": 52,
        "prasna": "क्व स्थातव्यम्",
        "utharam": "न्याय्ये पथि दृष्टादृष्टलाभाढ्ये"
    },
    {
        "sloka_number": "21",
        "question_number": 53,
        "prasna": "कोऽन्धः",
        "utharam": "योऽकार्यरतः"
    },
    {
        "sloka_number": "21",
        "question_number": 54,
        "prasna": "को बधिरः",
        "utharam": "यो हितानि न श्रुणोति"
    },
    {
        "sloka_number": "21",
        "question_number": 55,
        "prasna": "को मूकः",
        "utharam": "यः काले प्रियाणि वक्तुं न जानाति"
    },
    {
        "sloka_number": "22",
        "question_number": 56,
        "prasna": "किं दानम्",
        "utharam": "अनाकाङ्क्षम्"
    },
    {
        "sloka_number": "22",
        "question_number": 57,
        "prasna": "किं मित्रम्",
        "utharam": "यो निवारयति पापात्"
    },
    {
        "sloka_number": "22",
        "question_number": 58,
        "prasna": "कोऽलङ्कारः",
        "utharam": "शीलम्"
    },
    {
        "sloka_number": "22",
        "question_number": 59,
        "prasna": "किं वाचां मण्डनम्",
        "utharam": "सत्यम्"
    },
    {
        "sloka_number": "23",
        "question_number": 60,
        "prasna": "विद्युद्विलसितचपलं किम्",
        "utharam": "दुर्जनसङ्गतिर्युवतयश्च"
    },
    {
        "sloka_number": "23",
        "question_number": 61,
        "prasna": "कुलशीलनिष्प्रकम्पाः के कलिकालेऽपि",
        "utharam": "सज्जना एव"
    },
    {
        "sloka_number": "24",
        "question_number": 62,
        "prasna": "चिन्तामणिरिव दुर्लभमिह किम्",
        "utharam": "कथयामि तच्चतुर्भद्रम्"
    },
    {
        "sloka_number": "24-25",
        "question_number": 63,
        "prasna": "किं तद्वदन्ति भूयो विधुततमसो विशेषेण",
        "utharam": "दानं प्रियवाक्सहितं ज्ञानमगर्वं क्षमान्वितं शौर्यम् वित्तं त्यागसमेतं दुर्लभमेतच्चतुर्भद्रम्"
    },
    {
        "sloka_number": "26",
        "question_number": 64,
        "prasna": "किं शोच्यं",
        "utharam": "कार्पण्यं सति विभवे"
    },
    {
        "sloka_number": "26",
        "question_number": 65,
        "prasna": "किं प्रशस्तम्",
        "utharam": "औदार्यम्"
    },
    {
        "sloka_number": "26",
        "question_number": 66,
        "prasna": "कः पूज्यो विद्वद्भिः",
        "utharam": "स्वभावतः सर्वदा विनीतो यः"
    },
    {
        "sloka_number": "27",
        "question_number": 67,
        "prasna": "कः कुलकमलदिनेशः",
        "utharam": "सति गुणविभवेऽपि यो नम्रः"
    },
    {
        "sloka_number": "27",
        "question_number": 68,
        "prasna": "कस्य वशे जगदेतत्",
        "utharam": "प्रियहितवचनस्य धर्मनिरतस्य"
    },
    {
        "sloka_number": "28",
        "question_number": 69,
        "prasna": "विद्वन्मनोहरा का",
        "utharam": "सत्कविता बोधवनिता च"
    },
    {
        "sloka_number": "28",
        "question_number": 70,
        "prasna": "कं न स्पृशति विपत्तिः",
        "utharam": "प्रवृद्धवचनानुवर्तिनं दान्तम्"
    },
    {
        "sloka_number": "29",
        "question_number": 71,
        "prasna": "कस्मै स्पृहयति कमला",
        "utharam": "अनलसचित्ताय नीतिवृत्ताय"
    },
    {
        "sloka_number": "29",
        "question_number": 72,
        "prasna": "त्यजति च कं सहसा",
        "utharam": "द्विजगुरुसुरनिन्दाकरं च सालस्यम्"
    },
    {
        "sloka_number": "30",
        "question_number": 73,
        "prasna": "कुत्र विधेयो वासः",
        "utharam": "सज्जननिकटेऽथवा काश्याम्"
    },
    {
        "sloka_number": "30",
        "question_number": 74,
        "prasna": "कः परिहार्यो देशः",
        "utharam": "पिशुनयुतो लुब्धभूपश्च"
    },
    {
        "sloka_number": "31",
        "question_number": 75,
        "prasna": "केनाशोच्यः पुरुषः",
        "utharam": "प्रणतकलत्रेण धीरविभवेन"
    },
    {
        "sloka_number": "31",
        "question_number": 76,
        "prasna": "इह भुवने कः शोच्यः",
        "utharam": "सत्यपि विभवे न यो दाता"
    },
    {
        "sloka_number": "32",
        "question_number": 77,
        "prasna": "किं लघुताया मूलम्",
        "utharam": "प्राकृतपुरुषेषु या याच्ञा"
    },
    {
        "sloka_number": "32",
        "question_number": 78,
        "prasna": "रामादपि कः शूरः",
        "utharam": "स्मरशरनिहतो न यश्चलति"
    },
    {
        "sloka_number": "33",
        "question_number": 79,
        "prasna": "किमहर्निशमनुचिन्त्यम्",
        "utharam": "भगवच्चरणं न संसारः"
    },
    {
        "sloka_number": "33",
        "question_number": 80,
        "prasna": "चक्षुष्मन्तोऽप्यन्धाः के स्युः",
        "utharam": "ये नास्तिका मनुजाः"
    },
    {
        "sloka_number": "34",
        "question_number": 81,
        "prasna": "कः पङ्गुरिह",
        "utharam": "प्रथितो व्रजति च यो वार्द्धके तीर्थम्"
    },
    {
        "sloka_number": "34",
        "question_number": 82,
        "prasna": "किं तीर्थमपि च",
        "utharam": "मुख्यं चित्तमलं यन्निवर्तयति"
    },
    {
        "sloka_number": "35",
        "question_number": 83,
        "prasna": "किं स्मर्त्तव्यं पुरुषैः",
        "utharam": "हरिनाम सदा न यावनी भाषा"
    },
    {
        "sloka_number": "35",
        "question_number": 84,
        "prasna": "को हि न वाच्यः सुधिया ",
        "utharam": "परदोषश्चानृतं तद्वत्"
    },
    {
        "sloka_number": "36",
        "question_number": 85,
        "prasna": "किं सम्पाद्यं मनुजैः",
        "utharam": "विद्या वित्तं बलं यशः पुण्यम्"
    },
    {
        "sloka_number": "36",
        "question_number": 86,
        "prasna": "कः सर्वगुणविनाशी",
        "utharam": "लोभः"
    },
    {
        "sloka_number": "36",
        "question_number": 87,
        "prasna": "शत्रुश्च कः",
        "utharam": "कामः"
    },
    {
        "sloka_number": "37",
        "question_number": 88,
        "prasna": "का च सभा परिहार्या",
        "utharam": "हीना या वृद्धसचिवेन"
    },
    {
        "sloka_number": "37",
        "question_number": 89,
        "prasna": "इह कुत्रावहितः स्यान्मनुजः",
        "utharam": "किल राजसेवायाम्"
    },
    {
        "sloka_number": "38",
        "question_number": 90,
        "prasna": "प्राणादपि को रम्यः",
        "utharam": "कुलधर्मः साधुसङ्गश्च"
    },
    {
        "sloka_number": "38",
        "question_number": 91,
        "prasna": "का संरक्ष्या",
        "utharam": "कीर्तिः पतिव्रता नैजबुद्धिश्च"
    },
    {
        "sloka_number": "39",
        "question_number": 92,
        "prasna": "का कल्पलता लोके",
        "utharam": "सच्छिष्यायार्पिता विद्या"
    },
    {
        "sloka_number": "39",
        "question_number": 93,
        "prasna": "कोऽक्षयवटवृक्षः स्यात्",
        "utharam": "विधिवत्सत्पात्रदत्तदानं यत्"
    },
    {
        "sloka_number": "40",
        "question_number": 94,
        "prasna": "किं शास्त्रम्",
        "utharam": "सर्वेषां युक्तिः"
    },
    {
        "sloka_number": "40",
        "question_number": 95,
        "prasna": "माता च का",
        "utharam": "धेनुः"
    },
    {
        "sloka_number": "40",
        "question_number": 96,
        "prasna": "किं नु बलम्",
        "utharam": "यद्धैर्यम्"
    },
    {
        "sloka_number": "40",
        "question_number": 97,
        "prasna": "को मृत्युः",
        "utharam": "यदवधानरहितत्वम्"
    },
    {
        "sloka_number": "41",
        "question_number": 98,
        "prasna": "कुत्र विषम्",
        "utharam": "दुष्टजने"
    },
    {
        "sloka_number": "41",
        "question_number": 99,
        "prasna": "किमिहाशौचं भवेत् ",
        "utharam": "ऋणं नृणाम्"
    },
    {
        "sloka_number": "41",
        "question_number": 100,
        "prasna": "किमभयमिह",
        "utharam": "वैराग्यम्"
    },
    {
        "sloka_number": "41",
        "question_number": 101,
        "prasna": "भयमपि किम्",
