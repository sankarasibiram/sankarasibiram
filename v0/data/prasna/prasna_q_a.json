    },
    {
        "sloka_number": "65",
        "question_number": 178,
        "prasna": "कोऽर्च्यः",
        "utharam": "भगवदवतारः"
    },
    {
        "sloka_number": "65",
        "question_number": 179,
        "prasna": "कश्च भगवान्",
