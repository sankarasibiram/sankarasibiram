    },
    {
        "sloka_number": "60",
        "question_number": 164,
        "prasna": "वपुषश्च पोषकं किम्",
        "utharam": "प्रारब्धम्"
    },
    {
        "sloka_number": "60",
        "question_number": 165,
        "prasna": "अन्नदायि किम्",
        "utharam": "आयुः"
    },
    {
        "sloka_number": "61",
        "question_number": 166,
        "prasna": "को ब्राह्मणैरुपास्यः",
