    },
    {
        "sloka_number": "48",
        "question_number": 120,
        "prasna": "साधुबलं किम्",
        "utharam": "दैवम्"
    },
    {
        "sloka_number": "48",
        "question_number": 121,
        "prasna": "कः साधुः",
