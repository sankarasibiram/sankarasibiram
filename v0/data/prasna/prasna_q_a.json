    },
    {
        "sloka_number": "46",
        "question_number": 116,
        "prasna": "को वा हीयेत",
        "utharam": "यो दृप्तः"
    },
    {
        "sloka_number": "46",
        "question_number": 117,
        "prasna": "को न प्रत्येतव्यः",
        "utharam": "ब्रूते यश्चानृतं शश्वत्"
    },
    {
        "sloka_number": "47",
        "question_number": 118,
        "prasna": "कुत्रानृतेऽप्यपापम्",
