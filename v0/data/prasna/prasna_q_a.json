    },
    {
        "sloka_number": "54",
        "question_number": 141,
        "prasna": "को जगत्भर्ता",
        "utharam": "सूर्यः"
    },
    {
        "sloka_number": "54",
        "question_number": 142,
        "prasna": "सर्वेषां को जीवनहेतुः",
