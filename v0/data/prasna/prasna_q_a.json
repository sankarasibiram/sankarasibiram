    },
    {
        "sloka_number": "57",
        "question_number": 151,
        "prasna": "किं सुखम्",
        "utharam": "तुष्टिः"
    },
    {
        "sloka_number": "57",
        "question_number": 152,
        "prasna": "को राजा",
        "utharam": "रञ्जन​कृत्"
